		<item name="Wheat Sprout" count="3,5" loot_prob_template="high"/>
		<item name="Barley Sprout" count="3,5" loot_prob_template="high"/>
		<item name="Sugar Cane Sprout" count="3,5" loot_prob_template="high"/>
		<item name="Agave Sprout" count="3,5" loot_prob_template="high"/>
		</append>
		
