<StressedOut>

<insertAfter xpath="/lootcontainers/lootgroup[@name='groupDrinksRare']">
			<lootgroup name="craftbeers">
				<item name="Lager"/>
				<item name="Porter"/>
				<item name="Pilsner"/>
				<item name="Stout"/>
				<item name="Pale Ale"/>
				<item name="IPA"/>
				<item name="Pumpkin Ale"/>
				<item name="Wheat Ale"/>
				<item name="drinkJarBeer"/>
			</lootgroup>
</insertAfter>

<insertAfter xpath="/lootcontainers/lootgroup[@name='craftbeers']">
			<lootgroup name="liquors">
				<item name="Whiskey"/>
				<item name="Vodka"/>
				<item name="Rum"/>
				<item name="Gin"/>
				<item name="Tequila"/>
				<item name="Sake"/>
				<item name="Brandy"/>
			</lootgroup>
</insertAfter>

		
		<append xpath="/lootcontainers/lootcontainer[@name='cooler']">
		<item group="craftbeers" prob=".5" count="1,3"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='refrigerator']">
		<item group="craftbeers" prob=".4" count="2,4"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='garbage']">
		<item group="liquors" prob=".15" count="0,1"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='beerCooler']">
		<item group="craftbeers" prob=".5" count="2,4"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='cupboard']">
		<item group="liquors" prob=".3" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='woodenDesk']">
		<item group="liquors" prob=".2" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='shoppingCart']">
		<item group="liquors" prob=".2" count="0,1"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='shoppingBasket']">
		<item group="liquors" prob=".1" count="0,1"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='dumpster']">
		<item group="liquors" prob=".2" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='policeCars']">
		<item group="liquors" prob=".15" count="1,2"/>
		<item group="craftbeers" prob=".3" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='buriedSuppliesT1']">
		<item group="liquors" prob=".15" count="0,1"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='buriedSuppliesT2']">
		<item group="liquors" prob=".30" count="2,3"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='shamwayShelves']">
		<item group="liquors" prob=".1" count="0,1"/>
		<item group="craftbeers" prob=".3" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='zPackBoss']">
		<item group="liquors" prob=".1" count="0,1"/>
		<item group="craftbeers" prob=".3" count="1,2"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='groupLiquorPileSmall']">
		<item group="liquors" prob=".4" count="0,1"/>
		<item group="craftbeers" prob=".1" count="0,1"/>
		<item name="Wheat Sprout" count="1,2" loot_prob_template="low"/>
		<item name="Barley Sprout" count="1,2" loot_prob_template="low"/>
		<item name="Sugar Cane Sprout" count="1,2" loot_prob_template="low"/>
		<item name="Agave Sprout" count="1,2" loot_prob_template="low"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='groupLiquorPileMed']">
		<item group="liquors" prob=".5" count="1,2"/>
		<item group="craftbeers" prob=".1" count="0,2"/>
		<item name="Wheat Sprout" count="2,3" loot_prob_template="med"/>
		<item name="Barley Sprout" count="2,3" loot_prob_template="med"/>
		<item name="Sugar Cane Sprout" count="2,3" loot_prob_template="med"/>
		<item name="Agave Sprout" count="2,3" loot_prob_template="med"/>
		</append>
		
		<append xpath="/lootcontainers/lootcontainer[@name='groupLiquorPileLarge']">
		<item group="liquors" prob=".6" count="3,5"/>
